{
  "@id": "contract3",
  "@type": "mco-core:Contract",
  "label": "CONTRACT 3 - On demand stream - Self-released. For artist who own their sound recording copyrights and use services like CD Baby or TuneCore",
  "hasParty": [
    {
      "@id": "Consumer",
      "label": "CONSUMER",
      "@type": ["mco-core:Party", "mvco:EndUser"]
    },
    {
      "@id": "StreamingService",
      "label": "STREAMING SERVICE",
      "@type": ["mco-core:Party", "mvco:Distributor"]
    },
    {
      "@id": "PerformingRighthsOrganisation",
      "label": "PERFORMING RIGHTS ORGANISATION",
      "@type": ["mco-core:Party"]
    },
    { "@id": "Publisher", "label": "PUBLISHER", "@type": ["mco-core:Party"] },
    { "@id": "Aggregator", "label": "AGGREGATOR", "@type": ["mco-core:Party"] },
    { "@id": "Author", "label": "AUTHOR/BAND", "@type": ["mco-core:Party"] },
    {
      "@id": "MechanicalLicenseAgent",
      "label": "MECHANICAL LICENSE AGENT",
      "@type": ["mco-core:Party", "mvco:Instantiator"]
    }
  ],
  "issues": [
    {
      "@id": "permission1",
      "label": "Consumer can play a song",
      "@type": ["mvco:Permission"],
      "issuedBy": {
        "@id": "StreamingService",
        "label": "STREAMING SERVICE",
        "@type": ["mco-core:Party", "mvco:Distributor"]
      },
      "permitsAction": {
        "@id": "action1",
        "@type": ["mvco:play"],
        "label": "Consumer plays a song",
        "actedBy": {
          "@id": "Consumer",
          "label": "CONSUMER",
          "@type": ["mco-core:Party", "mvco:EndUser"]
        },
        "actedOver": { "@id": "Song", "label": "SONG", "@type": ["mvco:Work"] }
      }
    },
    {
      "@id": "permission1",
      "label": "Record label authorises streaming service",
      "@type": ["mvco:Permission"],
      "issuedBy": {
        "@id": "Aggregator",
        "label": "AGGREGATOR",
        "@type": ["mco-core:Party"]
      },
      "permitsAction": {
        "@id": "action2",
        "@type": ["mvco:distribute"],
        "label": "Service distributes a song",
        "actedBy": {
          "@id": "StreamingService",
          "label": "STREAMING SERVICE",
          "@type": ["mco-core:Party", "mvco:Distributor"]
        },
        "actedOver": { "@id": "Song", "label": "SONG", "@type": ["mvco:Work"] }
      }
    },
    {
      "@id": "permission1",
      "label": "Publisher authorises streaming service",
      "@type": ["mvco:Permission"],
      "issuedBy": {
        "@id": "Publisher",
        "label": "PUBLISHER",
        "@type": ["mco-core:Party"]
      },
      "permitsAction": {
        "@id": "action2",
        "@type": ["mvco:distribute"],
        "label": "Service distributes a song",
        "actedBy": {
          "@id": "StreamingService",
          "label": "STREAMING SERVICE",
          "@type": ["mco-core:Party", "mvco:Distributor"]
        },
        "actedOver": { "@id": "Song", "label": "SONG", "@type": ["mvco:Work"] }
      }
    },
    {
      "@id": "permission4",
      "label": "Author authorises aggregator to distribute",
      "@type": ["mvco:Permission"],
      "issuedBy": {
        "@id": "Author",
        "label": "AUTHOR/BAND",
        "@type": ["mco-core:Party"]
      },
      "permitsAction": {
        "@id": "action4",
        "@type": ["mvco:distribute"],
        "label": "Record label to distribute",
        "actedBy": {
          "@id": "Aggregator",
          "label": "AGGREGATOR",
          "@type": ["mco-core:Party"]
        },
        "actedOver": { "@id": "Song", "label": "SONG", "@type": ["mvco:Work"] }
      }
    },
    {
      "@id": "obligation1",
      "label": "Publisher provide a song to streaming",
      "@type": ["mco-core:Obligation"],
      "issuedBy": {
        "@id": "Publisher",
        "label": "PUBLISHER",
        "@type": ["mco-core:Party"]
      },
      "obligatesAction": {
        "@id": "action5",
        "@type": ["mvco-ipre:provideMaterial"],
        "label": "Streaming provides a song",
        "actedBy": {
          "@id": "Publisher",
          "label": "PUBLISHER",
          "@type": ["mco-core:Party"]
        },
        "actedOver": { "@id": "Song", "label": "SONG", "@type": ["mvco:Work"] },
        "actedTo": {
          "@id": "StreamingService",
          "label": "STREAMING SERVICE",
          "@type": ["mco-core:Party", "mvco:Distributor"]
        }
      }
    },
    {
      "@id": "obligation3",
      "label": "Consumer pays a fixed rate",
      "@type": ["mco-core:Obligation"],
      "issuedBy": {
        "@id": "Consumer",
        "label": "CONSUMER",
        "@type": ["mco-core:Party", "mvco:EndUser"]
      },
      "obligatesAction": {
        "@id": "pay1",
        "@type": ["mco-ipre:Payment"],
        "label": "Payment",
        "actedBy": {
          "@id": "Consumer",
          "label": "CONSUMER",
          "@type": ["mco-core:Party", "mvco:EndUser"]
        },
        "mco-pane:hasBeneficiary": {
          "@id": "StreamingService",
          "label": "STREAMING SERVICE",
          "@type": ["mco-core:Party", "mvco:Distributor"]
        },
        "mco-pane:hasAmount": 19,
        "hasCurrency": "EUR"
      }
    },
    {
      "@id": "obligation4",
      "label": "Streaming service pays 10% to publisher",
      "@type": ["mco-core:Obligation"],
      "issuedBy": {
        "@id": "StreamingService",
        "label": "STREAMING SERVICE",
        "@type": ["mco-core:Party", "mvco:Distributor"]
      },
      "obligatesAction": {
        "@id": "pay2",
        "@type": ["mco-ipre:Payment"],
        "label": "Payment",
        "actedBy": {
          "@id": "StreamingService",
          "label": "STREAMING SERVICE",
          "@type": ["mco-core:Party", "mvco:Distributor"]
        },
        "mco-pane:hasBeneficiary": {
          "@id": "MechanicalLicenseAgent",
          "label": "MECHANICAL LICENSE AGENT",
          "@type": ["mco-core:Party", "mvco:Instantiator"]
        },
        "mco-pane:hasIncomePercentage": 10
      }
    },
    {
      "@id": "obligation5",
      "label": "Streaming service pays 1% to PRO",
      "@type": ["mco-core:Obligation"],
      "issuedBy": {
        "@id": "StreamingService",
        "label": "STREAMING SERVICE",
        "@type": ["mco-core:Party", "mvco:Distributor"]
      },
      "obligatesAction": {
        "@id": "pay3",
        "@type": ["mco-ipre:Payment"],
        "label": "Payment",
        "actedBy": {
          "@id": "StreamingService",
          "label": "STREAMING SERVICE",
          "@type": ["mco-core:Party", "mvco:Distributor"]
        },
        "mco-pane:hasBeneficiary": {
          "@id": "PerformingRighthsOrganisation",
          "label": "PERFORMING RIGHTS ORGANISATION",
          "@type": ["mco-core:Party"]
        },
        "mco-pane:hasIncomePercentage": 1
      }
    },
    {
      "@id": "obligation6",
      "label": "Streaming service must pay 50% to Aggregator",
      "@type": ["mco-core:Obligation"],
      "issuedBy": {
        "@id": "StreamingService",
        "label": "STREAMING SERVICE",
        "@type": ["mco-core:Party", "mvco:Distributor"]
      },
      "obligatesAction": {
        "@id": "pay4",
        "@type": ["mco-ipre:Payment"],
        "label": "Payment",
        "actedBy": {
          "@id": "StreamingService",
          "label": "STREAMING SERVICE",
          "@type": ["mco-core:Party", "mvco:Distributor"]
        },
        "mco-pane:hasBeneficiary": {
          "@id": "Aggregator",
          "label": "AGGREGATOR",
          "@type": ["mco-core:Party"]
        },
        "mco-pane:hasIncomePercentage": 50
      }
    },
    {
      "@id": "obligation7",
      "label": "Aggregator must pay the author",
      "@type": ["mco-core:Obligation"],
      "issuedBy": {
        "@id": "StreamingService",
        "label": "STREAMING SERVICE",
        "@type": ["mco-core:Party", "mvco:Distributor"]
      },
      "obligatesAction": {
        "@id": "pay5",
        "@type": ["mco-ipre:Payment"],
        "label": "Payment",
        "actedBy": {
          "@id": "Aggregator",
          "label": "AGGREGATOR",
          "@type": ["mco-core:Party"]
        },
        "mco-pane:hasBeneficiary": {
          "@id": "Author",
          "label": "AUTHOR/BAND",
          "@type": ["mco-core:Party"]
        },
        "mco-pane:hasIncomePercentage": 90
      }
    },
    {
      "@id": "obligation8",
      "label": "Publisher must pay 10-50% to Author",
      "@type": ["mco-core:Obligation"],
      "issuedBy": {
        "@id": "StreamingService",
        "label": "STREAMING SERVICE",
        "@type": ["mco-core:Party", "mvco:Distributor"]
      },
      "obligatesAction": {
        "@id": "pay6",
        "@type": ["mco-ipre:Payment"],
        "label": "Payment",
        "actedBy": {
          "@id": "Publisher",
          "label": "PUBLISHER",
          "@type": ["mco-core:Party"]
        },
        "mco-pane:hasBeneficiary": {
          "@id": "Author",
          "label": "AUTHOR/BAND",
          "@type": ["mco-core:Party"]
        },
        "mco-pane:hasIncomePercentage": 10
      }
    },
    {
      "@id": "obligation9",
      "label": "Mechanical License Agent must pay to Publisher",
      "@type": ["mco-core:Obligation"],
      "issuedBy": {
        "@id": "StreamingService",
        "label": "STREAMING SERVICE",
        "@type": ["mco-core:Party", "mvco:Distributor"]
      },
      "obligatesAction": {
        "@id": "pay7",
        "@type": ["mco-ipre:Payment"],
        "label": "Payment",
        "actedBy": {
          "@id": "MechanicalLicenseAgent",
          "label": "MECHANICAL LICENSE AGENT",
          "@type": ["mco-core:Party", "mvco:Instantiator"]
        },
        "mco-pane:hasBeneficiary": {
          "@id": "Publisher",
          "label": "PUBLISHER",
          "@type": ["mco-core:Party"]
        },
        "mco-pane:hasIncomePercentage": 50
      }
    }
  ],
  "@context": {
    "@base": "http://mpeg.org/",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "owl": "http://www.w3.org/2002/07/owl#",
    "mvco": "http://purl.oclc.org/NET/mvco.owl#",
    "mco-core": "urn:mpeg:mpeg21:mco:core:2012#",
    "mco-ipre": "urn:mpeg:mpeg21:mco:ipre:2012#",
    "mco-pane": "urn:mpeg:mpeg21:mco:pane:2015#",
    "issuedBy": {
      "@id": "mvco:issuedBy",
      "@type": "@id"
    },
    "actedBy": {
      "@id": "mvco:actedBy",
      "@type": "@id"
    },
    "hasParty": {
      "@id": "mco-core:hasParty",
      "@container": "@set"
    },
    "actedOver": {
      "@id": "mvco:actedOver",
      "@type": "@id"
    },
    "issuedIn": {
      "@id": "mco-core:issuedIn",
      "@type": "@id"
    },
    "permitsAction": {
      "@id": "mco-core:permitsAction",
      "@type": "@id"
    },
    "obligatesAction": {
      "@id": "mco-core:obligatesAction",
      "@type": "@id"
    },
    "hasRequired": {
      "@id": "mco-core:hasRequired",
      "@type": "@id"
    },
    "label": {
      "@id": "rdfs:label",
      "@type": "xsd:string"
    },
    "issues": {
      "@id": "mco-core:issues",
      "@container": "@set"
    },
    "hasCurrency": {
      "@id": "mco-pane:hasCurency",
      "@type": "xsd:string"
    },
    "hasAmount": {
      "@id": "mco-pane:hasAmount",
      "@type": "xsd:integer"
    }
  }
}
